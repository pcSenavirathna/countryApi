{
	"version": 2,
	"builds": [
	  {
		"src": "countryinfoweb/package.json",
		"use": "@vercel/static-build",
		"config": {
		  "distDir": "build"
		}
	  },
	  {
		"src": "backend/server.js",
		"use": "@vercel/node"
	  }
	],
	"routes": [
	  {
		"src": "/api/(.*)",
		"dest": "/backend/server.js"
	  },
	  {
		"src": "/(.*)",
		"dest": "/countryinfoweb/build/$1"
	  }
	]
  }